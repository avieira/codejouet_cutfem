#include "Polygon2D.h"
#include "my_real.h"

int main(){
    my_real x_p[4] = {0.5, 1.5, 1.5, 0.5};
    my_real y_p[4] = {0.5, 0.5, 1.5, 1.5};
    const unsigned long nb_pts = 4;

    GrB_init(GrB_NONBLOCKING);
    Polygon2D* p = polygon_from_consecutive_points(x_p, y_p, nb_pts);
    GrB_finalize();
}